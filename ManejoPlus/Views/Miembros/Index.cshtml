@model IEnumerable<Miembro>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreMiembro)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EmailOpcional)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rol)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MontoAportado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ApplicationUserId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Suscripcion)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreMiembro)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EmailOpcional)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Rol)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MontoAportado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApplicationUserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Suscripcion.ApplicationUserId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MiembroID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MiembroID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MiembroID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
